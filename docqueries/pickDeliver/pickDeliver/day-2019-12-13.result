/* save stops after optimize */
WITH
b AS (
    SELECT vehicle_id AS id, array_remove(array_agg(shipment_id ORDER BY seq)::BIGINT[],-1::BIGINT) AS stops
    FROM the_stops
    GROUP BY vehicle_id
)
UPDATE vehicles AS v
SET stops = b.stops
FROM b WHERE v.id = b.id;
